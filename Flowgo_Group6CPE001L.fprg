                                <else>
                                    <if expression="cmenuchoice = 2">
                                        <then/>
                                        <else>
                                            <if expression="cmenuchoice = 3">
                                                <then/>
                                                <else>
                                                    <if expression="cmenuchoice = 4">
                                                        <then/>
                                                        <else>
                                                            <if expression="cmenuchoice = 5">
                                                                <then/>
